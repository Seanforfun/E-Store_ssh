<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
        <hibernate-mapping>
        	<class name="ca.mcmaster.shop.admin.Admin" table="admin">
        		<id name="admin_id">
        			<generator class="native"/>
        		</id>
        		
        		<property name="admin_name" length="50"/>
        		<property name="admin_password" length="50"/>
        	</class>
        </hibernate-mapping>